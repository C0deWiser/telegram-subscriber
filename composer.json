{
    "name": "codewiser/telegram-subscriber",
    "description": "Telegram Subscriber for Laravel",
    "keywords": ["Laravel", "Subscriber", "Telegram"],
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Codewiser\\Telegram\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.0",
        "irazasyed/telegram-bot-sdk": "^3.0",
        "laravel/framework": ">=10.0"
    },
    "suggest": {
        "codewiser/laravel-notifications": "Unified contract for notificaion messages of any type",
        "laravel-notification-channels/telegram": "Telegram Notifications Channel for Laravel"
    },
    "authors": [
        {
            "name": "Cellard",
            "email": "pm@101media.ru"
        }
    ],
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers" : [
                "Codewiser\\Telegram\\TelegramServiceProvider"
            ]
        }
    },
    "require-dev": {
        "codewiser/laravel-notifications": "^1.1",
        "phpunit/phpunit": "^11.1"
    }
}
